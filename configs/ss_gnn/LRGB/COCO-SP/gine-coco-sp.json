{
    "name":"SubgraphSampling: COCO-SP SS GINE",
    "dataset_name": "COCO-SP",
    "model_name": "SS-GNN",
    "task": "Node-Classification",

    "model_config":
    {
        "name": "ss_gnn",
        "mpnn_type": "gine",
        "node_feature_dim": 14,
        "edge_feature_dim": 2,
        "hidden_dim": 220,
        "mpnn_layers": 8,
        "dropout": 0.0,
        "out_dim": 81,
        "pooling": "mean",
        "subgraph_sampling": true,
        "subgraph_param": {
            "k": 8,
            "m": 100,
            "pooling": "mean"
        }

    },

    "train":
    {
        "epochs": 500,
        "train_batch_size": 32,
        "val_batch_size": 32,
        "lr": 0.0005,
        "weight_decay": 0.0,
        "optimizer": "adamw",
        "scheduler": {
            "type":"reduce_on_plateau",
            "patience":10
        },
        "loss_fn": "CrossEntropyLoss",
        "metric": "F1"
    },

    "device":"cuda",
    "seed": 42,
    "num_workers": 4,
    "log_dir": "logs",
    "checkpoint_dir": "checkpoints",
    "cache_dir": "cache",
    "save_every": 1,
    "keep_last_k": 3,

    "sampler": "ugs",
    "use_amp": false,
    "grad_clip": 2.0
}
