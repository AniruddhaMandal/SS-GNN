  {
    "name": "CSL: SS-GNN (k=6, GIN)",
    "dataset_name": "CSL",
    "model_name": "SS-GNN",
    "task": "Multi-Class-Classification",
    "model_config": {
      "name": "ss_gnn",
      "mpnn_type": "gin",
      "node_feature_dim": 5,
      "hidden_dim": 64,
      "mpnn_layers": 4,
      "dropout": 0.0,
      "out_dim": 10,
      "pooling": "attention",
      "temperature": 0.20,
      "subgraph_sampling": true,
      "subgraph_param": {"k": 6, "m": 300, "pooling": "mean"},
      "kwargs": {"node_feature_type": "all_one"}
    },
    "train": {
      "epochs": 1000,
      "train_batch_size": 4,
      "val_batch_size": 4,
      "train_ratio": 0.6,
      "val_ratio": 0.2,
      "lr": 1e-3,
      "weight_decay": 1e-5,
      "optimizer": "adam",
      "scheduler": {"type": "reduce_on_plateau", "patience": 50},
      "loss_fn": "CrossEntropyLoss",
      "metric": "ACC"
    },
    "device": "cuda",
    "seed": 42,
    "num_workers": 0,
    "log_dir": "logs",
    "checkpoint_dir": "checkpoints/ss-csl-k6/",
    "cache_dir": "cache",
    "sampler": "uniform",
    "use_amp": false,
    "tracker": "wandb"
  }