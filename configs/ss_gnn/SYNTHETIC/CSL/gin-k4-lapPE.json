{
  "name": "CSL: SS-GNN (k=4, GIN, LapPE)",
  "dataset_name": "CSL",
  "model_name": "SS-GNN",
  "task": "Multi-Class-Classification",
  "model_config": {
    "name": "ss_gnn",
    "mpnn_type": "gin",
    "node_feature_dim": 8,
    "hidden_dim": 64,
    "mpnn_layers": 4,
    "dropout": 0.0,
    "out_dim": 10,
    "pooling": "attention",
    "temperature": 0.20,
    "subgraph_sampling": true,
    "subgraph_param": {"k": 4, "m": 200, "pooling": "mean"},
    "kwargs": {
      "node_feature_type": "lap_pe",
      "lap_pe_dim": 8
    }
  },
  "train": {
    "epochs": 500,
    "train_batch_size": 4,
    "val_batch_size": 4,
    "train_ratio": 0.6,
    "val_ratio": 0.2,
    "lr": 5e-4,
    "weight_decay": 1e-5,
    "optimizer": "adam",
    "scheduler": {"type": "reduce_on_plateau", "patience": 30},
    "loss_fn": "CrossEntropyLoss",
    "metric": "ACC"
  },
  "device": "cuda",
  "seed": 42,
  "num_workers": 0,
  "log_dir": "logs",
  "checkpoint_dir": "checkpoints/ss-csl-k4-gin-lapPE/",
  "cache_dir": "cache",
  "sampler": "uniform",
  "use_amp": false,
  "tracker": "Off"
}
