{
  "name": "SS-GNN: PROTEINS VANILLA GIN",
  "dataset_name": "PROTEINS",
  "model_name": "SS-GNN",
  "task": "Binary-Classification",

  "model_config": {
    "name": "ss_gnn",
    "mpnn_type": "gin",
    "node_feature_dim": 3,
    "hidden_dim": 64,
    "mpnn_layers": 4,
    "dropout": 0.6,
    "out_dim": 2,
    "pooling": "attention",
    "temperature": 0.5,
    "subgraph_sampling": true,
    "subgraph_param": {
        "k": 8,
        "m": 100,
        "pooling": "sum"
    }
  },

  "train": {
    "epochs": 150,
    "train_batch_size": 32,
    "val_batch_size": 32,
    "train_ratio": 0.7,
    "val_ratio": 0.15,
    "lr": 1e-3,
    "weight_decay": 5e-4,
    "optimizer": "adam",
    "scheduler": {
      "type": "reduce_on_plateau",
      "patience": 20
    },
    "loss_fn": "CrossEntropyLoss",
    "metric": "ACC"
  },

  "device": "cuda",
  "seed": 42,
  "num_workers": 4,
  "log_dir": "logs/proteins",
  "checkpoint_dir": "checkpoints",
  "cache_dir": "cache",
  "save_every": 1,
  "keep_last_k": 3,

  "use_amp": false,
  "grad_clip": 1.0
}
