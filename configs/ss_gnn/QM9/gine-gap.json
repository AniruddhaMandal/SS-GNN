{
  "name": "SS-GNN: QM9-Gap GINE",
  "dataset_name": "QM9",
  "model_name": "SS-GNN",
  "task": "Single-Target-Regression",

  "model_config": {
    "name": "ss_gnn",
    "mpnn_type": "gine",
    "node_feature_dim": 11,
    "edge_feature_dim": 4,
    "hidden_dim": 256,
    "mpnn_layers": 5,
    "dropout": 0.0,
    "out_dim": 1,
    "pooling": "attention",
    "temperature": 0.5,
    "subgraph_sampling": true,
    "subgraph_param": {
      "k": 5,
      "m": 50,
      "pooling": "sum"
    }
  },

  "train": {
    "epochs": 50,
    "train_batch_size": 96,
    "val_batch_size": 96,
    "train_ratio": 0.8,
    "val_ratio": 0.1,
    "lr": 1e-3,
    "weight_decay": 0.0,
    "optimizer": "adam",
    "scheduler": {
      "type": "reduce_on_plateau",
      "patience": 25,
      "factor": 0.5
    },
    "loss_fn": "L1Loss",
    "metric": "MAE",
    "dataloader_kwargs": {
      "target_idx": 4,
      "normalize_target": false
    }
  },

  "device": "cuda",
  "seed": 42,
  "num_workers": 4,
  "log_dir": "logs/qm9",
  "checkpoint_dir": "checkpoints",
  "cache_dir": "cache",
  "save_every": 10,
  "keep_last_k": 3,

  "use_amp": false,
  "grad_clip": 0.0
}
