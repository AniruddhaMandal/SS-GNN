{
    "name": "VANILLA: AmazonPhoto GCN",
    "task": "Node-Classification",
    "dataset_name": "AmazonPhoto",
    "model_name": "VANILLA",
    "model_config": {
        "mpnn_type": "gcn",
        "node_feature_dim": 745,
        "hidden_dim": 64,
        "out_dim": 8,
        "mpnn_layers": 3,
        "dropout": 0.5,
        "pooling": "off",
        "kwargs": {
            "residual": false
        }
    },
    "train": {
        "epochs": 200,
        "train_batch_size": 1,
        "val_batch_size": 1,
        "lr": 0.01,
        "weight_decay": 0.0005,
        "optimizer": "adam",
        "loss_fn": "CrossEntropyLoss",
        "metric": "ACC",
        "train_ratio": 0.6,
        "val_ratio": 0.2
    },
    "device": "cuda",
    "seed": 42,
    "num_workers": 0,
    "cache_dir": "./cache"
}