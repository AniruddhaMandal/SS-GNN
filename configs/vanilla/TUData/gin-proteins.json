{
  "name": "Baseline: PROTEINS VANILLA GIN",
  "dataset_name": "PROTEINS",
  "model_name": "VANILLA",
  "task": "Binary-Classification",

  "model_config": {
    "name": "vanilla",
    "mpnn_type": "gin",
    "node_feature_dim": 3,
    "hidden_dim": 128,
    "mpnn_layers": 5,
    "dropout": 0.2,
    "out_dim": 2,
    "pooling": "mean"
  },

  "train": {
    "epochs": 100,
    "train_batch_size": 32,
    "val_batch_size": 32,
    "train_ratio": 0.8,
    "val_ratio": 0.2,
    "lr": 1e-3,
    "weight_decay": 1e-5,
    "optimizer": "adam",
    "scheduler": {
      "type": "reduce_on_plateau",
      "patience": 20
    },
    "loss_fn": "CrossEntropyLoss",
    "metric": "ACC"
  },

  "device": "cuda",
  "seed": 42,
  "num_workers": 4,
  "log_dir": "logs/proteins",
  "checkpoint_dir": "checkpoints",
  "cache_dir": "cache",
  "save_every": 1,
  "keep_last_k": 3,

  "use_amp": false,
  "grad_clip": 2.0
}
