{
    "name":"SS-GNN-WL: MUTAG SS GCN(k=5)",
    "dataset_name": "MUTAG",
    "model_name": "SS-GNN-WL",
    "task": "Binary-Classification",

    "model_config": 
    {
        "name": "ss_gnn_wl",
        "mpnn_type": "gcn",
        "node_feature_dim": 7,
        "hidden_dim": 64,
        "mpnn_layers": 4,
        "dropout": 0.1,
        "out_dim": 2,
        "pooling": "sum",
        "temperature": 0.5,
        "subgraph_sampling": true,
        "subgraph_param": {
            "k": 5,
            "m": 100,
            "pooling": "mean"
        },
        "kwargs": {
            "wl_vocab_path": "wl_vocabs/mutag.pkl",
            "wl_dim": 64,
            "use_node_features_in_wl": true,
            "wl_iterations": 3
        }

    },

    "train": 
    {
        "epochs": 50,
        "train_batch_size": 64,
        "val_batch_size": 64,
        "lr": 1e-3,
        "weight_decay": 1e-5,
        "optimizer": "adam",  
        "scheduler": {
            "type":"reduce_on_plateau",
            "patience":20
        },
        "loss_fn": "CrossEntropyLoss",
        "metric": "ACC"
    },

    "device":"cuda",
    "seed": 42,
    "num_workers": 4,
    "log_dir": "logs",
    "checkpoint_dir": "checkpoints",
    "cache_dir": "cache",
    "save_every": 1,
    "keep_last_k": 3,

    "use_amp": false,
    "grad_clip": 2.0,
    "sampler": "uniform"
}