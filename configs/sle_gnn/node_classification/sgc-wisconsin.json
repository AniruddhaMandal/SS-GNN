{
    "name": "SLE-GNN: Wisconsin SGC",
    "task": "Node-Classification",
    "dataset_name": "Wisconsin",
    "model_name": "SLE-GNN",

    "model_config": {
        "mpnn_type": "sgc",
        "node_feature_dim": 1703,
        "hidden_dim": 64,
        "out_dim": 5,
        "mpnn_layers": 3,
        "dropout": 0.5,
        "pooling": "off",
        "kwargs": {
            "residual": true,
            "jk_mode": null
        }
    },

    "train": {
        "epochs": 200,
        "train_batch_size": 1,
        "val_batch_size": 1,
        "lr": 0.01,
        "weight_decay": 0.0005,
        "optimizer": "adam",
        "loss_fn": "CrossEntropyLoss",
        "metric": "ACC",
        "train_ratio": 0.6,
        "val_ratio": 0.2
    },

    "device": "cuda",
    "seed": 42,
    "num_workers": 0,
    "cache_dir": "./cache"
}
